      </div>
    </nav>
    <!-- End of navbar -->

    <!-- Yield to page -->
    <div class="content">
      <%= yield %>
    </div>
    <!-- End yield to page -->

    <!-- Footer -->
    <footer class="footer text-right">
      <div class="container">
        <ul class="social_media">
          <li><a href="https://www.linkedin.com/in/matthewyerkes/" target="_blank"><%= image_tag "/images/linkedin_logo.png", style: 'width: 30px; margin: 5px;' %></a></li>
          <li><a href="https://github.com/myerkes" target="_blank"><%= image_tag "/images/github_logo.png", style: 'width: 30px; margin: 5px;' %></a></li>
          <li><a href="https://www.hackerrank.com/Mattyerkes23" target="_blank" title="HackerRank">HackerRank</a></li>        
        </ul>
      
        <p>Â© 2021 Matt Yerkes</p>
